createSection("üòà 4. Face Animation Exploits")

local faceSpamming = false
createToggle("Spam Face Animations", function(state)
    faceSpamming = state
    if state and Events then
        task.spawn(function()
            local AnimateFace = Events:FindFirstChild("AnimateFace")
            if not AnimateFace then return end
            
            local faces = {
                "rbxassetid://53769373",
                "rbxassetid://20418518",
                "rbxassetid://67493660",
                "rbxassetid://105880745"
            }
            
            while faceSpamming do
                pcall(function()
                    AnimateFace:FireServer(faces[math.random(1, #faces)])
                end)
                task.wait(0.01) -- Super fast spam
            end
        end)
        updateStatus("Face Spam ON", Color3.fromRGB(255, 100, 100))
    else
        updateStatus("Face Spam OFF", Color3.fromRGB(100, 255, 100))
    end
end)

-- ===================================
-- SECTION 5: INPUT MANIPULATION
-- ===================================
createSection("üñ±Ô∏è 5. Input Manipulation")

local autoClicking = false
createToggle("Auto-Clicker", function(state)
    autoClicking = state
    if state then
        task.spawn(function()
            while autoClicking do
                pcall(function()
                    -- Simulate mouse click
                    local VirtualInputManager = game:GetService("VirtualInputManager")
                    VirtualInputManager:SendMouseButtonEvent(0, 0, 0, true, game, 0)
                    task.wait(0.01)
                    VirtualInputManager:SendMouseButtonEvent(0, 0, 0, false, game, 0)
                end)
                task.wait(0.05)
            end
        end)
        updateStatus("Auto-Clicker ON", Color3.fromRGB(255, 100, 100))
    else
        updateStatus("Auto-Clicker OFF", Color3.fromRGB(100, 255, 100))
    end
end)

createButton("Bypass Mobile Touch Detection", function()
    updateStatus("Touch Detection Bypassed", Color3.fromRGB(255, 100, 100))
    
    -- Modify camera viewport
    if workspace.CurrentCamera then
        local cam = workspace.CurrentCamera
        pcall(function()
            cam.ViewportSize = Vector2.new(99999, 99999)
        end)
    end
end)

-- ===================================
-- SECTION 6: BLOCK MANIPULATION
-- ===================================
createSection("üß± 6. Block & World Manipulation")

createButton("Mass Place Blocks (Lag)", function()
    if not Events then return end
    updateStatus("Mass Placing Blocks...", Color3.fromRGB(255, 200, 50))
    
    local RequestPlaceBlock = Events:FindFirstChild("RequestPlaceBlock")
    if RequestPlaceBlock and LocalPlayer.Character then
        local hrp = LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
        if hrp then
            for x = -20, 20, 3 do
                for y = -20, 20, 3 do
                    for z = -20, 20, 3 do
                        pcall(function()
                            RequestPlaceBlock:FireServer(
                                hrp.Position + Vector3.new(x, y, z),
                                "Block"
                            )
                        end)
                    end
                end
            end
            updateStatus("Mass Place Complete!", Color3.fromRGB(100, 255, 100))
        end
    end
end)

createButton("Destroy All Nearby Blocks", function()
    if not Events then return end
    updateStatus("Destroying Blocks...", Color3.fromRGB(255, 100, 100))
    
    local PunchBlock = Events:FindFirstChild("PunchBlock")
    if PunchBlock and LocalPlayer.Character then
        local hrp = LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
        if hrp then
            -- Find all blocks near player
            local region = Region3.new(hrp.Position - Vector3.new(50, 50, 50), hrp.Position + Vector3.new(50, 50, 50))
            region = region:ExpandToGrid(4)
            
            for _, part in pairs(workspace:FindPartsInRegion3(region, nil, 100)) do
                pcall(function()
                    if part.Parent:IsA("Model") then
                        PunchBlock:FireServer(part.Parent)
                    end
                end)
                task.wait()
            end
            updateStatus("Destruction Complete!", Color3.fromRGB(100, 255, 100))
        end
    end
end)

-- ===================================
-- SECTION 7: EXPLOIT UTILITIES
-- ===================================
createSection("üõ†Ô∏è 7. Utility Functions")

createButton("Teleport to Mouse Position", function()
    if LocalPlayer.Character then
        local hrp = LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
        local mouse = LocalPlayer:GetMouse()
        if hrp and mouse.Hit then
            hrp.CFrame = CFrame.new(mouse.Hit.Position + Vector3.new(0, 3, 0))
            updateStatus("Teleported!", Color3.fromRGB(100, 255, 100))
        end
    end
end)

createButton("Noclip (Toggle)", function()
    local noclipping = false
    if LocalPlayer.Character then
        noclipping = not noclipping
        local connection
        connection = RunService.Stepped:Connect(function()
            if noclipping and LocalPlayer.Character then
                for _, part in pairs(LocalPlayer.Character:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = false
                    end
                end
            else
                if connection then connection:Disconnect() end
            end
        end)
        updateStatus(noclipping and "Noclip ON" or "Noclip OFF", 
            noclipping and Color3.fromRGB(255, 100, 100) or Color3.fromRGB(100, 255, 100))
    end
end)

createButton("Remove All Fog/Effects", function()
    pcall(function()
        game.Lighting.FogEnd = 999999
        game.Lighting.Brightness = 2
        for _, effect in pairs(game.Lighting:GetChildren()) do
            if effect:IsA("Atmosphere") or effect:IsA("BlurEffect") or effect:IsA("ColorCorrectionEffect") then
                effect:Destroy()
            end
        end
        updateStatus("Fog Removed!", Color3.fromRGB(100, 255, 100))
    end)
end)

createButton("Walkspeed Boost (16 -> 100)", function()
    if LocalPlayer.Character then
        local humanoid = LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            humanoid.WalkSpeed = 100
            updateStatus("Speed Boosted!", Color3.fromRGB(100, 255, 100))
        end
    end
end)

