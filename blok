createSection("üëë 2. Admin Bypass Exploits")

createButton("Force Admin Status (Client)", function()
    updateStatus("Attempting Admin Bypass...", Color3.fromRGB(255, 200, 50))
    
    -- Try to hook IsPlayerOwnerOrAdmin function
    local success = pcall(function()
        local worldData = workspace:FindFirstChild("WorldData")
        if worldData then
            local worldOwner = worldData:FindFirstChild("WorldOwner")
            if worldOwner then
                worldOwner.Value = LocalPlayer.Name
                updateStatus("Set as World Owner (Client)", Color3.fromRGB(255, 100, 100))
            end
        end
    end)
    
    if not success then
        updateStatus("Admin Bypass Failed", Color3.fromRGB(255, 50, 50))
    end
end)

createButton("Spam Admin Actions", function()
    if not Events then return end
    
    updateStatus("Spamming Admin Events...", Color3.fromRGB(255, 100, 100))
    
    for i = 1, 50 do
        pcall(function()
            local RequestWrench = Events:FindFirstChild("RequestWrench")
            if RequestWrench then
                RequestWrench:FireServer("AdminAction", true)
            end
        end)
        task.wait(0.05)
    end
    
    updateStatus("Admin Spam Complete", Color3.fromRGB(100, 255, 100))
end)

