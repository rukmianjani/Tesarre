        game:GetService("GuiService").ErrorMessageChanged:Connect(function()
            task.wait(1)
            TeleportService:Teleport(game.PlaceId, LocalPlayer)
        end)
        updateStatus("Anti-Kick Active", Color3.fromRGB(100, 255, 100))
    end
end)

createToggle("Hide Name Tag", function(state)
    if LocalPlayer.Character then
        local head = LocalPlayer.Character:FindFirstChild("Head")
        if head then
            for _, tag in pairs(head:GetChildren()) do
                if tag:IsA("BillboardGui") then
                    tag.Enabled = not state
                end
            end
        end
        updateStatus(state and "Name Hidden" or "Name Visible", 
            state and Color3.fromRGB(100, 255, 100) or Color3.fromRGB(255, 200, 50))
    end
end)

createButton("Clear All Connections", function()
    -- Clear all script connections to prevent detection
    updateStatus("Clearing connections...", Color3.fromRGB(255, 200, 50))
    
    pcall(function()
        for _, connection in pairs(getconnections(game.LogService.MessageOut)) do
            connection:Disable()
        end
    end)
    
    updateStatus("Connections Cleared", Color3.fromRGB(100, 255, 100))
end)

