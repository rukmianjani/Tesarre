        if BlockModule and BlockModule.blocks then
            for itemName, itemData in pairs(BlockModule.blocks) do
                -- Try to give item locally
                if LocalPlayer:FindFirstChild("LastSelectedItem") then
                    LocalPlayer.LastSelectedItem.Value = itemName
                    updateStatus("Selected: " .. itemName, Color3.fromRGB(100, 255, 100))
                    break
                end
            end
        end
    end)
end)

