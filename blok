-- SECTION 3: RAYCAST MANIPULATION
-- ===================================
createSection("üéØ 3. Raycast & Detection Bypass")

createToggle("Click Through Walls", function(state)
    if state then
        -- Hook workspace:Raycast
        local oldRaycast = workspace.Raycast
        workspace.Raycast = function(self, origin, direction, params)
            -- Return fake result at max range
            return {
                Instance = workspace,
                Position = origin + (direction.Unit * 999),
                Normal = Vector3.new(0, 1, 0),
                Distance = 999
            }
        end
        updateStatus("Wallhack Raycast ON", Color3.fromRGB(255, 100, 100))
    else
        updateStatus("Raycast Normal", Color3.fromRGB(100, 255, 100))
    end
end)

createToggle("Bypass GUI Detection", function(state)
    if state and LocalPlayer.PlayerGui then
        -- Make all GUI transparent or remove them
        for _, gui in pairs(LocalPlayer.PlayerGui:GetDescendants()) do
            if gui:IsA("Frame") or gui:IsA("ImageButton") then
                pcall(function()
                    gui.BackgroundTransparency = 1
                end)
            end
        end
        updateStatus("GUI Bypass Active", Color3.fromRGB(255, 100, 100))
    else
        updateStatus("GUI Normal", Color3.fromRGB(100, 255, 100))
    end
end)

