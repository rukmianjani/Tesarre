createSection("üòà 10. Player Trolling Tools")

local targetPlayer = nil
createButton("Select Target Player (Click)", function()
    local mouse = LocalPlayer:GetMouse()
    updateStatus("Click on a player to target...", Color3.fromRGB(255, 200, 50))
    
    local connection
    connection = mouse.Button1Down:Connect(function()
        local target = mouse.Target
        if target and target.Parent and target.Parent:FindFirstChild("Humanoid") then
            local player = Players:GetPlayerFromCharacter(target.Parent)
            if player then
                targetPlayer = player
                updateStatus("Targeted: " .. player.Name, Color3.fromRGB(255, 100, 100))
                connection:Disconnect()
            end
        end
    end)
end)

createButton("Cage Target Player", function()
    if not targetPlayer or not targetPlayer.Character then
        updateStatus("No target selected!", Color3.fromRGB(255, 50, 50))
        return
    end
    
    if not Events then return end
    
    local hrp = targetPlayer.Character:FindFirstChild("HumanoidRootPart")
    if hrp and Events:FindFirstChild("RequestPlaceBlock") then
        updateStatus("Caging " .. targetPlayer.Name, Color3.fromRGB(255, 100, 100))
        
        local RequestPlaceBlock = Events:FindFirstChild("RequestPlaceBlock")
        local pos = hrp.Position
        
        -- Build cage around player
        for x = -6, 6, 3 do
            for y = -3, 9, 3 do
                for z = -6, 6, 3 do
                    if math.abs(x) == 6 or math.abs(z) == 6 or y == -3 or y == 9 then
                        pcall(function()
                            RequestPlaceBlock:FireServer(pos + Vector3.new(x, y, z), "Block")
                        end)
                        task.wait(0.01)
                    end
                end
            end
        end
        updateStatus("Cage Complete!", Color3.fromRGB(100, 255, 100))
    end
end)

createButton("Spam Blocks at Target", function()
    if not targetPlayer or not targetPlayer.Character then
        updateStatus("No target selected!", Color3.fromRGB(255, 50, 50))
        return
    end
    
    if not Events then return end
    
    local hrp = targetPlayer.Character:FindFirstChild("HumanoidRootPart")
    if hrp and Events:FindFirstChild("RequestPlaceBlock") then
        updateStatus("Spamming at " .. targetPlayer.Name, Color3.fromRGB(255, 100, 100))
        
        task.spawn(function()
            for i = 1, 200 do
                pcall(function()
                    local RequestPlaceBlock = Events:FindFirstChild("RequestPlaceBlock")
                    local offset = Vector3.new(math.random(-15, 15), math.random(-5, 10), math.random(-15, 15))
                    RequestPlaceBlock:FireServer(hrp.Position + offset, "Block")
                end)
                task.wait(0.02)
            end
            updateStatus("Spam Complete!", Color3.fromRGB(100, 255, 100))
        end)
    end
end)

