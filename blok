                        item.CFrame = hrp.CFrame
                    end)
                end
            end
            if FloatingGems then
                for _, gem in pairs(FloatingGems:GetChildren()) do
                    pcall(function()
                        gem.CFrame = hrp.CFrame
                    end)
                end
            end
            updateStatus("Items Collected!", Color3.fromRGB(100, 255, 100))
        end
    end
end)

