createSection("‚ö° 8. Advanced Techniques")

createButton("Exploit All Vulnerabilities", function()
    updateStatus("Activating All Exploits...", Color3.fromRGB(255, 50, 50))
    
    -- Remove cooldowns
    if LocalPlayer.Character then
        LocalPlayer.Character:SetAttribute("LocalCooldown", nil)
    end
    
    -- Set infinite range
    if LocalPlayer.Character then
        local char = LocalPlayer.Character
        if char:FindFirstChild("EquippedFace") then
            char.EquippedFace:SetAttribute("RangeMultiplier", 999999)
        end
        if char:FindFirstChild("EquippedHand") then
            char.EquippedHand:SetAttribute("RangeMultiplier", 999999)
        end
    end
    
    -- Speed boost
    if LocalPlayer.Character then
        local humanoid = LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            humanoid.WalkSpeed = 100
        end
    end
    
    -- Remove fog
    pcall(function()
        game.Lighting.FogEnd = 999999
    end)
    
    updateStatus("ALL EXPLOITS ACTIVE!", Color3.fromRGB(255, 0, 0))
    
    -- Continuous exploit maintenance
    task.spawn(function()
        while task.wait(1) do
            pcall(function()
                if LocalPlayer.Character then
                    LocalPlayer.Character:SetAttribute("LocalCooldown", nil)
                end
            end)
        end
    end)
end)

createButton("Lag Server (Spam All Events)", function()
    updateStatus("‚ö†Ô∏è LAGGING SERVER...", Color3.fromRGB(255, 0, 0))
    
    task.spawn(function()
        for i = 1, 1000 do
            pcall(function()
                if Events then
                    for _, event in pairs(Events:GetChildren()) do
                        if event:IsA("RemoteEvent") then
                            event:FireServer("SPAM", math.random(), {}, true, Vector3.new())
                        end
                    end
                end
            end)
            task.wait()
        end
        updateStatus("Server Lag Complete", Color3.fromRGB(100, 255, 100))
    end)
end)

createButton("Hook All RemoteEvents", function()
    local hooked = {}
    for _, event in pairs(ReplicatedStorage:GetDescendants()) do
        if event:IsA("RemoteEvent") and not hooked[event] then
            hooked[event] = true
            local oldFireServer = event.FireServer
            event.FireServer = function(self, ...)
                print("üî¥ [HOOKED]", event:GetFullName(), "fired with args:", ...)
                return oldFireServer(self, ...)
            end
        end
    end
    updateStatus("All RemoteEvents Hooked!", Color3.fromRGB(255, 200, 50))
end)

